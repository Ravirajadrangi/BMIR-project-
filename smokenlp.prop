trainFile=smoke-train-stanford-classifier.txt
testFile=smoke-test-stanford-classifier.txt
1.useSplitWords=true
1.splitWordsTokenizerRegexp=M[ -`]{60} |[\\p{L}][\\p{L}0-9]*|(?:\\$ ?)?[0-9]+(?:\\.[0-9]{2})?%?|\\s+|[\\x80-\\uFFFD]|.
1.splitWordsIgnoreRegexp=M[ -`]{60} |\\s+
1.useSplitPrefixSuffixNGrams=true
1.maxNGramLeng=4
1.minNGramLeng=1
1.splitWordShape=chris4
printClassifier=HighWeight
printClassifierParam=200
printTo=propoutput.txt
sigma=3
crossValidationFolds=10